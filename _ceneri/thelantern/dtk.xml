<docuteck>
  <el name="command" des="insieme di istruzioni e parole chiave che insieme danno un ordine a theLantern">
    <el name="command action" des="azione principale del comando"></el>
    <el name="command object" des="oggetto principale del comando"></el>
    <el name="sub-command" des="istruzione secondaria del comando"></el>
    <el name="command sub-object" des="oggetto secondario del comando"></el>
  </el>
  <el name="user">
    <el name="user type" type="attributo" des="tipo di utente">
      <values>
        <val>administrator</val>
        <val>normal</val>
      </values>
    </el>
    <el name="user name" type="attributo" des="nome univoco utente"/>
    <el name="user email" type="attributo" des="email utente"/>
  </el>
  <el name="html block">
  </el>
  <el name="node" des="contenitore generico di oggetti">
    <el name="node parent id" type="attributo" des="identificatore univoco del nodo contenitore"/>
    <el name="node id" type="attributo" des="identificatore univoco del nodo"/>
    <el name="node title" type="attributo" des="titolo visualizzato per il nodo"/>
    <el name="node notes" type="attributo" des="descrizione nodo"/>
    <el name="node objects" type="lista" des="lista degli oggetti contenuti"/>
    <el name="child nodes" type="lista" des="lista dei nodi contenuti"/>
  </el>
  <el name="class" des="definizione classi degli elementi che popolano i nodi">
    <el name="class id" type="attributo" des="identificatore univoco classe"/>
    <el name="class name" type="attributo" des="nome univoco della classe"/>
    <el name="class notes" type="attributo" des="breve descrizione classe"/>
    <el name="class details" type="attributo" des="descrizione completa e dettagliata della classe"/>
  </el>
  <el name="object" des="singolo oggetto contenuto in un nodo">
    <el name="object node id" type="attributo" des="identificatore univoco del nodo contenitore dell'oggetto"/>
    <el name="object class id" type="attributo" des="identificatore univoco classe di definizione dell'oggetto"/>
    <el name="object id" type="attributo" des="identificatore univoco dell'oggetto"/>
  </el>
</docuteck>
